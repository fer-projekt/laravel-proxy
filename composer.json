{
  "name": "fer-projekt/laravel-proxy",
  "type": "library",
  "description": "Laravel proxy trait to resolve the currently bound values of the given contract.",
  "keywords": [
    "laravel",
    "proxy"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0|^8.0",
    "illuminate/container": "^9.0|^10.0",
    "illuminate/support": "^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "FerProjekt\\LaravelProxy\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FerProjekt\\LaravelProxy\\Tests\\": "tests/src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "FerProjekt\\LaravelProxy\\LaravelProxyServiceProvider"
      ]
    }
  }
}